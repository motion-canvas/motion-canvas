{
  "name": "@motion-canvas/2d",
  "version": "3.11.0",
  "description": "A 2D renderer for Motion Canvas",
  "author": "motion-canvas",
  "homepage": "https://motioncanvas.io/",
  "bugs": "https://github.com/motion-canvas/motion-canvas/issues",
  "license": "MIT",
  "main": "lib/runtime/index.js",
  "types": "./lib/runtime/index.d.ts",
  "exports": {
    ".": "./lib/runtime/index.js",
    "./lib": "./lib/runtime/index.js",
    "./lib/components": "./lib/runtime/components/index.js",
    "./lib/curves": "./lib/runtime/curves/index.js",
    "./lib/decorators": "./lib/runtime/decorators/index.js",
    "./lib/partials": "./lib/runtime/partials/index.js",
    "./lib/scenes": "./lib/runtime/scenes/index.js",
    "./lib/utils": "./lib/runtime/utils/index.js",
    "./lib/*": {
      "import": "./lib/runtime/*.js",
      "default": "./lib/runtime/*"
    },
    "./editor": "./lib/editor/index.js",
    "./tsconfig.project.json": "./tsconfig.project.json"
  },
  "scripts": {
    "dev": "tspc -b src -w",
    "build": "tspc -b src",
    "bundle": "rollup -c rollup.config.mjs",
    "test": "vitest"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motion-canvas/motion-canvas.git"
  },
  "files": [
    "lib",
    "src",
    "tsconfig.project.json"
  ],
  "devDependencies": {
    "@motion-canvas/internal": "0.0.0",
    "jsdom": "^22.1.0",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@motion-canvas/core": "^3.11.0",
    "code-fns": "^0.8.2",
    "mathjax-full": "^3.2.2",
    "parse-svg-path": "^0.1.2"
  }
}
